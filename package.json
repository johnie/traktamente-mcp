{
  "name": "traktamente-mcp",
  "version": "1.1.0",
  "description": "MCP server for Swedish traktamente (per diem) rates from Skatteverket",
  "type": "module",
  "private": false,
  "main": "dist/index.js",
  "bin": {
    "traktamente-mcp": "dist/index.js"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "traktamente",
    "per-diem",
    "skatteverket",
    "sweden",
    "travel-expenses"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnie/traktamente-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/johnie/traktamente-mcp/issues"
  },
  "homepage": "https://github.com/johnie/traktamente-mcp#readme",
  "author": "Johnie Hjelm <johnie@hjelm.im>",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "start": "bun src/index.ts",
    "start:http": "TRANSPORT=http bun src/index.ts",
    "dev": "bun --watch src/index.ts",
    "dev:hot": "bun --hot src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun --minify --sourcemap --external @modelcontextprotocol/sdk",
    "build:worker": "bun build src/worker.ts --outdir=dist --target=bun --minify --sourcemap --external @modelcontextprotocol/sdk",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "inspector": "bunx @modelcontextprotocol/inspector bun src/index.ts",
    "docker:build": "docker build -t traktamente-mcp .",
    "docker:run": "docker run -i traktamente-mcp",
    "workers:dev": "wrangler dev",
    "workers:deploy": "wrangler deploy",
    "workers:tail": "wrangler tail",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@hono/mcp": "0.2.2",
    "@hono/zod-validator": "0.7.5",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "hono": "4.11.1",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@cloudflare/workers-types": "^4.20251213.0",
    "@modelcontextprotocol/inspector": "^0.17.5",
    "@types/bun": "latest",
    "lefthook": "2.0.12",
    "typescript": "^5.9.3",
    "ultracite": "6.4.1",
    "wrangler": "^4.54.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
